{
    "name": "mirko-pagliai/me-cms",
    "description": "MeCms plugin for CakePHP",
    "homepage": "https://github.com/mirko-pagliai/me-cms",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [{
        "name": "Mirko Pagliai",
        "email": "mirko.pagliai@gmail.com"
    }],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.5 <4",
        "donatj/phpuseragentparser": "*",
        "enyo/dropzone": "^5.1",
        "gourmet/common-mark": "~1.0",
        "js-cookie/js-cookie": "v2.1.3",
        "mirko-pagliai/cakephp-database-backup": ">=2.2 <2.3",
        "mirko-pagliai/cakephp-recaptcha-mailhide":  ">=1.1 <1.2",
        "mirko-pagliai/cakephp-thumber": ">=1.3 <1.4",
        "mirko-pagliai/cakephp-tokens": ">=1.1 <1.2",
        "mirko-pagliai/me-tools": ">=2.16.4-RC2 <2.17",
        "mirko-pagliai/serialized-array": "^1.0",
        "wyrihaximus/minify-html": "^1.1",
        "sunra/php-simple-html-dom-parser": "^1.5",
        "crabstudio/recaptcha": "^2.0"
    },
    "suggest": {
        "mirko-pagliai/me-cms-instagram": "Instagram plugin for MeCms",
        "mirko-pagliai/me-cms-youtube": "Youtube plugin for MeCms",
        "sunhater/kcfinder:dev-master": "To use KCFinder to browse and upload files"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^3.0",
        "mirko-pagliai/reflection": "^1.0",
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "MeCms\\": "src"
        },
        "files": [
            "src/Core/global_functions.php",
            "src/Core/request_detectors.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MeCms\\Test\\": "tests",
            "App\\": "tests/test_app/TestApp",
            "TestPlugin\\": "tests/test_app/TestApp/Plugin/TestPlugin/src",
            "TestPluginTwo\\": "tests/test_app/TestApp/Plugin/TestPluginTwo/src"
        }
    },
    "minimum-stability": "beta"
}
