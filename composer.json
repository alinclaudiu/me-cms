{
    "name": "mirko-pagliai/me-cms",
    "description": "MeCms plugin for CakePHP",
    "homepage": "https://github.com/mirko-pagliai/me-cms",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [{
        "name": "Mirko Pagliai",
        "email": "mirko.pagliai@gmail.com"
    }],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "^3.7.1",
        "crabstudio/recaptcha": "^2.0",
        "donatj/phpuseragentparser": "*",
        "enyo/dropzone": "^5.1",
        "gourmet/common-mark": "^1.0",
        "mirko-pagliai/cakephp-database-backup": "^2.5",
        "mirko-pagliai/cakephp-entity-file-log": "^1.0",
        "mirko-pagliai/cakephp-recaptcha-mailhide": "^1.2",
        "mirko-pagliai/cakephp-stop-spam": "^1.0.1-beta2",
        "mirko-pagliai/cakephp-thumber": "^1.5.2",
        "mirko-pagliai/cakephp-tokens": "^1.2",
        "mirko-pagliai/me-tools": "^2.18.7",
        "mirko-pagliai/php-tools": "^1.2",
        "npm-asset/js-cookie": "^v2.2",
        "wyrihaximus/minify-html": "^1.1"
    },
    "require-dev": {
        "cakephp/debug_kit": "~3.0",
        "cakephp/cakephp-codesniffer": "^3.0",
        "mirko-pagliai/cakephp-assets": "^1.3",
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "suggest": {
        "mirko-pagliai/me-cms-instagram": "Instagram plugin for MeCms",
        "mirko-pagliai/me-cms-link-scanner": "Link scanner plugin for MeCms",
        "sunhater/kcfinder:dev-master": "To use KCFinder to browse and upload files"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "MeCms\\": "src"
        },
        "files": [
            "src/Core/global_functions.php",
            "src/Core/request_detectors.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MeCms\\Test\\": "tests",
            "App\\": "tests/test_app/TestApp/",
            "TestPlugin\\": "tests/test_app/TestApp/Plugin/TestPlugin/src",
            "TestPluginTwo\\": "tests/test_app/TestApp/Plugin/TestPluginTwo/src"
        }
    }
}
