{
    "name": "mirko-pagliai/me-cms",
    "description": "MeCms plugin for CakePHP",
    "homepage": "https://github.com/mirko-pagliai/me-cms",
    "type": "cakephp-plugin",
    "license": "AGPL",
    "authors": [
        {
            "name": "Mirko Pagliai",
            "email": "mirko.pagliai@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.4 <4.0",
        "js-cookie/js-cookie": "dev-master",
        "enyo/dropzone": "^4.3",
        "donatj/phpuseragentparser": "*",
        "mirko-pagliai/assets": "1.1.*",
        "mirko-pagliai/database-backup": ">=1.1.4",
        "mirko-pagliai/me-tools": ">=2.10.1",
        "mirko-pagliai/thumbs": "1.0.*",
        "wyrihaximus/minify-html": "^1.1",
        "gourmet/common-mark": "~1.0"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "js-cookie/js-cookie",
                "version": "v2.1.2",
                "source": {
                    "url": "https://github.com/js-cookie/js-cookie.git",
                    "type": "git",
                    "reference": "v2.1.2"
                }
            }
        }
    ],
    "require-dev": {
        "cakephp/cakephp-codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "MeCms\\": "src"
        },
        "files": [
            "src/Core/global_functions.php",
            "src/Core/request_detectors.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MeCms\\Test\\": "tests",
            "TestPlugin\\": "tests/test_app/Plugin/TestPlugin/src",
            "TestPluginTwo\\": "tests/test_app/Plugin/TestPluginTwo/src"
        }
    }
}
